	public Figura () {
		super();
		this.alto = 0.0;
		this.ancho = 0.0;
	}
	
	public Figura (String color, double alto, double ancho) {
		this
	}
	
	@Override
	public double perimetro() {
		return 0.0;
	}
	
}
