package figuras;

public class Rectangulo extends Figura{

	private double alto;
	private double ancho;
	
	public Rectangulo() {
		super();
		this.alto = 0.0;
		this.ancho = 0.0;
	}
	public Rectangulo(String color, double alto, double ancho) {
		super(color);
		this.alto = alto;
		this.ancho = ancho;
	}
	
}
