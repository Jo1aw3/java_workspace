package pizzeria_v04;

public class Pizza {

	private int id;
	private String nombre;
	private String ingredientes;
	private double precio;
	
	public Pizza(int id, String nombre, String ingredientes, double precio) {
		this.id = id;
		this.nombre = nombre;
		this.ingredientes = ingredientes;
		this.precio = precio;
	}
	
}
