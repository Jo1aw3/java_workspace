package usuarios;

public class Usuario {

	private int id;
	private String nombre;
	private String apellido;
	private String username;
	private String password;
	private boolean activo;
	
	public Usuario() {
		this.id = 0;
		this.nombre = "";
		this.apellido = "";
		this.username = "";
		this.password = "";
		this.activo = false;
	}

	public Usuario(int id, String nombre, String apellido, String username, String password, boolean activo) {
		this.id = id;
		this.nombre = nombre;
		this.apellido = apellido;
		this.username = username;
		this.password = password;
		this.activo = activo;
	}
	
}
